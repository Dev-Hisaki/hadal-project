 (squareX === maxX - 2) {","                        shapePainter.drawRectangle(first15SquareX, squareY, squareX + 1, squareY + 1);","                        first15SquareX = -1;","                    }","                }","                if (squareIndex !== 0 && squareIndex !== 15) {","                    let fillVertices = marchingSquaresFillVertices[squareIndex];","                    this.calcPoint(fillVertices[0], squareX, squareY, threshold, point);","                    shapePainter.beginPath(point[0], point[1]);","                    for (let index = 1; index < fillVertices.length; index++) {","                        this.calcPoint(fillVertices[index], squareX, squareY, threshold, point);","                        shapePainter.lineTo(point[0], point[1]);","                    }","                    shapePainter.closePath();","                    shapePainter.endPath(squareX, squareY);","                }","            }","        }","    }","    /**","     * Draw the field squares","     * @param minX {integer} left","     * @param minY {integer} top","     * @param maxX {integer} right","     * @param maxY {integer} bottom","     * @param threshold {float}","     * @param drawUnder {boolean}","     * @param shapePainter {ShapePainter}","     */","    outlineContour(minX, minY, maxX, maxY, threshold, drawUnder, shapePainter) {","        const point = this.workingPoint;","        for (let squareY = minY; squareY < maxY - 1; squareY++) {","            for (let squareX = minX; squareX < maxX - 1; squareX++) {","                let squareIndex = this.getSquareIndex(squareX, squareY, threshold);","                if (drawUnder) {","                    squareIndex = 15 - squareIndex;","                }","                if (squareIndex !== 0 && squareIndex !== 15) {","                    for (let outlineVertices of marchingSquaresOutlineVertices[squareIndex]) {","                        this.calcPoint(outlineVertices[0], squareX, squareY, threshold, point);","